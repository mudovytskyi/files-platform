{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@*": [
				"./src/*"
			]
		},
		"jsx": "preserve",
		"plugins": [
			{
				"name": "next"
			}
		],
		"allowJs": true,
		"noEmit": true,
		"incremental": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"target": "es5",
		"lib": [
			"dom",
			"dom.iterable",
			"esnext"
		],
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts"
	],
	"exclude": [
		"node_modules"
	]
}
