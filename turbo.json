{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				"dist/**"
			]
		},
		"lint": {
			"outputs": []
		},
		"format": {
			"outputs": []
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"test": {
			"dependsOn": [
				"build"
			],
			"outputs": []
		}
	}
}